{
  "name": "ureport",
  "prettyName": "uReport",
  "company": "City of Bloomington, Indiana - ITS",
  "version": "1.1.10",
  "description": "Use uReport to notify the City of Bloomington, Indiana of community issues, such as potholes, graffiti, malfunctioning street lights, and more.",
  "author": "Adam Butcher - City of Bloomington, Indiana <butcherad@bloomington.in.gov>",
  "repository": {
    "type": "git",
    "url": "https://github.com/City-of-Bloomington/open311-nodejs.git"
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon server.js",
    "build": "nuxt build",
    "start": "nuxt build && nuxt start",
    "startLocal": "NODE_ENV=production nodemon server.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.9.5",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.2.2",
    "@nuxtjs/recaptcha": "^0.6.2",
    "@nuxtjs/style-resources": "^1.0.0",
    "axios": ">=0.21.1",
    "body-parser": "^1.19.0",
    "busboy-body-parser": "^0.3.2",
    "cheerio": "^1.0.0-rc.3",
    "cob-design-system": "git+https://github.com/City-of-Bloomington/design-system.git",
    "dotenv": "^6.2.0",
    "exif-js": "^2.3.0",
    "express": "^4.17.3",
    "nuxt": "^2.14.7",
    "proj4": "^2.6.0",
    "request": "^2.88.0",
    "vue-clickaway": "^2.2.2",
    "vue-meta": "^2.3.2",
    "vue2-google-maps": "^0.10.7",
    "vuex-map-fields": "^1.3.6",
    "vuex-persistedstate": "^2.7.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^7.3.1"
  }
